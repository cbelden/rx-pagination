<div class="paging-summary">Showing {{#unless singlePage}}{{startIndex}} - {{/unless}}{{endIndex}} of {{totalSize}} results</div>
{{#unless singlePage}}
<ul class="page-numbers">
    {{#each pageNumbers}}
    <li>
    {{#unless active}}{{number}}{{/unless}}
    {{#if active}}<a href="#">{{number}}</a>{{/if}}
    </li>
    {{/each}}
</ul>
{{/unless}}
